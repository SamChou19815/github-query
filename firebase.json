{
  "hosting": [
    {
      "target": "github-query",
      "public": "build",
      "ignore": ["**/.*"],
      "rewrites": [
        {
          "source": "/api/query",
          "function": "GithubQueryHandleClientRequest"
        },
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  ]
}
